<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="100.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ProjectController">
   <children>
      <VBox layoutX="11.0" layoutY="10.0" prefHeight="80.0" prefWidth="280.0">
         <children>
            <Label fx:id="label_projectName" alignment="CENTER" prefHeight="17.0" prefWidth="284.0" text="Project's Name" textAlignment="CENTER" textFill="#00918c" />
            <TextField fx:id="textfield_projectName" alignment="CENTER" promptText="Project's Name" style="-fx-background-radius: 5px; -fx-border-radius: 5px;">
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </TextField>
            <Label fx:id="label_notify" alignment="CENTER" prefHeight="25.0" prefWidth="280.0" textAlignment="CENTER" textFill="#f21616">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </children>
</AnchorPane>
